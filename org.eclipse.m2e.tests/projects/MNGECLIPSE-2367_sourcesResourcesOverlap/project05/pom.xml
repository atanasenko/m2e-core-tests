  <groupId>MNGECLIPSE-2367_sourcesResourcesOverlap</groupId>
  <artifactId>project05</artifactId>
  <version>1.0</version>

  <build>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <includes>
